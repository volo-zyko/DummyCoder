      <element name="property">
        <ref name="shdCstm_CT_Property"/>
      </element>
    </zeroOrMore>
  </define>
  <define name="shdCstm_CT_Property">
    <attribute name="fmtid">
      <ref name="s_ST_Guid"/>
    </attribute>
    <attribute name="pid">
